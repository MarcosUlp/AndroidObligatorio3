<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.obligatorio3.ui.listar.ListarFragment"
        android:label="Listar"
        tools:layout="@layout/fragment_listar" />

    <fragment
        android:id="@+id/nav_gallery"
        android:name="com.example.obligatorio3.ui.cargar.CargarFragment"
        android:label="Cargar"
        tools:layout="@layout/fragment_cargar" />

    <fragment
        android:id="@+id/nav_eliminar"
        android:name="com.example.obligatorio3.ui.Eliminar.EliminarFragment"
        android:label="Eliminar"
        tools:layout="@layout/fragment_eliminar" />
        <action
            android:id="@+id/action_nav_eliminar_to_detalleProducto"
            app:destination="@id/nav_detalleProducto" />

    <fragment
        android:id="@+id/nav_detalleProducto"
        android:name="com.example.obligatorio3.ui.Eliminar.DetalleProductoFragment"
        android:label="Detalle del Producto"
        tools:layout="@layout/fragment_detalle_producto" />

    <fragment
        android:id="@+id/nav_slideshow"
        android:name="com.example.obligatorio3.ui.Salir.SalirFragment"
        android:label="Salir"
        tools:layout="@layout/fragment_salir" />
</navigation>